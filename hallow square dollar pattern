import java.util.*;
class As111 
{
public static void main(String[] args) 
{
int row,col;
Scanner sc = new Scanner(System.in);
System.out.println("Enter row and col");
row = sc.nextInt();
col = sc.nextInt();
for (int i = 1; i <=row; i++) {
for (int j = 1; j <= col; j++) 
if((i==1 || i==col) || (j==1 || j==col))
System.out.print("$");
else
System.out.print(" ");			
System.out.println();			
}
}
}
